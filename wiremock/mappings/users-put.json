{
  "request": {
    "method": "PUT",
    "urlPathTemplate": "/users/{id}",
    "pathParameters": {
      "id": {
        "matches": "\\d+"
      }
    }
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "users-put-response.json",
    "transformers": [
      "response-template"
    ]
  }
}
